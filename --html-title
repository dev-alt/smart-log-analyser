<script>
    
    document.addEventListener('DOMContentLoaded', function() {
        
        if (typeof Chart === 'undefined') {
            console.error('Chart.js failed to load');
            return;
        }
        
        
        Chart.defaults.responsive = true;
        Chart.defaults.maintainAspectRatio = false;
        
        console.log('Chart.js loaded successfully, version:', Chart.version);
    
    
    console.log('Creating trafficChart...');
    const trafficCtx = document.getElementById('trafficChart').getContext('2d');
    const trafficChart = new Chart(trafficCtx, {
        type: 'pie',
        data: {
            labels: ['Human Traffic', 'Bot Traffic'],
            datasets: [{
                data: [ 2608 ,  6632 ],
                backgroundColor: ['#28a745', '#ffc107'],
                borderWidth: 2,
                borderColor: '#fff'
            }]
        },
        options: {
            plugins: {
                legend: { position: 'bottom' }
            }
        }
    });
    console.log('trafficChart created successfully');
    
    
    console.log('Creating hourlyChart...');
    const hourlyCtx = document.getElementById('hourlyChart').getContext('2d');
    const hourlyChart = new Chart(hourlyCtx, {
        type: 'line',
        data: {
            labels: ["00:00","01:00","02:00","03:00","04:00","05:00","06:00","07:00","08:00","09:00","10:00","11:00","12:00","13:00","14:00","15:00","16:00","17:00","18:00","19:00","20:00","21:00","22:00","23:00",],
            datasets: [{
                label: 'Requests',
                data: [ 211 , 217 , 275 , 776 , 989 , 921 , 911 , 936 , 938 , 695 , 347 , 691 , 205 , 173 , 189 , 185 , 63 , 75 , 106 , 61 , 97 , 59 , 52 , 68 ,],
                borderColor: '#667eea',
                backgroundColor: 'rgba(102, 126, 234, 0.1)',
                fill: true,
                tension: 0.4
            }]
        },
        options: {
            scales: {
                y: { beginAtZero: true }
            }
        }
    });
    console.log('hourlyChart created successfully');
    
    
    console.log('Creating statusChart...');
    const statusCtx = document.getElementById('statusChart').getContext('2d');
    const statusChart = new Chart(statusCtx, {
        type: 'doughnut',
        data: {
            labels: ["2xx Success","3xx Redirect","4xx Client Error",],
            datasets: [{
                data: [ 9071 , 99 , 70 ,],
                backgroundColor: ['#28a745', '#17a2b8', '#ffc107', '#dc3545'],
                borderWidth: 2,
                borderColor: '#fff'
            }]
        },
        options: {
            plugins: {
                legend: { position: 'bottom' }
            }
        }
    });
    console.log('statusChart created successfully');
    
    
    console.log('Creating responseSizeChart...');
    const responseSizeCtx = document.getElementById('responseSizeChart').getContext('2d');
    const responseSizeChart = new Chart(responseSizeCtx, {
        type: 'bar',
        data: {
            labels: ['P50', 'P95', 'P99', 'Average'],
            datasets: [{
                label: 'Response Size (KB)',
                data: [ 4.4521484375 ,  13.5224609375 ,  13.5244140625 ,  6.705078125 ],
                backgroundColor: ['#17a2b8', '#ffc107', '#fd7e14', '#6f42c1'],
                borderWidth: 1
            }]
        },
        options: {
            scales: {
                y: { beginAtZero: true }
            }
        }
    });
    console.log('responseSizeChart created successfully');
    
    
    console.log('Creating geoChart...');
    const geoCtx = document.getElementById('geoChart').getContext('2d');
    const geoChart = new Chart(geoCtx, {
        type: 'bar',
        data: {
            labels: ["Local\/Private","CDN\/Cloud","Unknown",],
            datasets: [{
                label: 'Requests',
